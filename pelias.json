{
  "imports": {
    "adminLookup" : {
      "enabled" : true
    },
    "transit": {
      "datapath": "/mnt/pelias/transit",
      "feeds": [
        {
          "type" : "stops",
          "filename" : "TRIMET-stops.txt",
          "agencyId" : "TRIMET"
        },
        {
          "type" : "stops",
          "filename" : "C-TRAN-stops.txt",
          "agencyId" : "C-TRAN"
        }
      ]
    },
    "openstreetmap": {
      "datapath": "/mnt/pelias/openstreetmap",
      "leveldbpath": "/tmp",
      "import": [{
        "filename": "or-wa.pbf"
      }]
    },
    "openaddresses": {
      "deduplicate": false,
      "datapath": "/mnt/pelias/openaddresses",
      "files": [
        "us/or/city_of_salem.csv",
        "us/or/gresham.csv",
        "us/or/marion_and_polk.csv",
        "us/or/oregon_city.csv",
        "us/or/washington.csv",
        "us/or/clackamas.csv",
        "us/or/hood_river.csv",
        "us/or/multnomah.csv",
        "us/or/portland.csv",
        "us/or/yarnhill.csv",
        "us/wa/city_of_richland.csv",
        "us/wa/clark.csv"
      ]
    },
    "polyline": {
      "datapath": "/mnt/pelias/polyline",
      "files": ["road_network"]
    },
    "whosonfirst": {
      "datapath": "/mnt/pelias/whosonfirst",
      "importVenues": false
    },
    "geonames": {
      "datapath": "./data",
      "countryCode": "US"
    }
  }
}
