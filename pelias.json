{
  "api": {
    "textAnalyzer": "libpostal"
  },
  "interpolation": {
    "client": {
      "timeout": 2000,
      "adapter": "http",
      "host": "http://127.0.0.1:3000"
    }
  },
  "imports": {
    "adminLookup" : {
      "enabled" : true
    },
    "transit": {
      "datapath": "/mnt/pelias/transit",
      "feeds": [
        {
          "layerId" : "landmarks",
          "filename" : "TRIMET-landmarks.csv",
          "agencyId"   : "TRIMET",
          "agencyName" : "TriMet"
        },
        {
          "layerId" : "intersections",
          "filename" : "intersections.csv"
        },
        {
          "url" : "x",
          "layerId" : "stops",
          "filename" : "TRIMET-stops.txt",
          "agencyId"   : "TRIMET",
          "agencyName" : "TriMet"
        },
        {
          "layerId" : "stops",
          "filename" : "C-TRAN-stops.txt",
          "agencyId"   : "C-TRAN",
          "agencyName" : "C-TRAN"
        },
        {
          "layerId" : "stops",
          "filename" : "RIDECONNECTION-stops.txt",
          "agencyId"   : "RIDECONNECTION",
          "agencyName" : "Ride Connection"
        },
        {
          "layerId" : "stops",
          "filename" : "CHERRIOTS-stops.txt",
          "agencyId"   : "CHERRIOTS",
          "agencyName" : "Cherriots / Salem-Keizer Transit"
        },
        {
          "layerId" : "stops",
          "filename" : "SAM-stops.txt",
          "agencyId"   : "SAM",
          "agencyName" : "Sandy Area Metro"
        },
        {
          "layerId" : "stops",
          "filename" : "SMART-stops.txt",
          "agencyId"   : "SMART",
          "agencyName" : "SMART"
        },
        {
          "layerId" : "stops",
          "filename" : "SWAN-stops.txt",
          "agencyId"   : "SWAN",
          "agencyName" : "Swan Island Evening Shuttle Service"
        }
      ]
    },
    "openstreetmap": {
      "datapath": "/mnt/pelias/openstreetmap",
      "leveldbpath": "/tmp",
      "import": [{
        "filename": "or-wa.pbf"
      }]
    },
    "openaddresses": {
      "deduplicate": false,
      "datapath": "/mnt/pelias/openaddresses",
      "files": [
        "us/or/city_of_salem.csv",
        "us/or/gresham.csv",
        "us/or/marion_and_polk.csv",
        "us/or/oregon_city.csv",
        "us/or/washington.csv",
        "us/or/clackamas.csv",
        "us/or/hood_river.csv",
        "us/or/multnomah.csv",
        "us/or/portland.csv",
        "us/or/yarnhill.csv",
        "us/wa/city_of_richland.csv",
        "us/wa/clark.csv"
      ]
    },
    "polyline": {
      "datapath": "/mnt/pelias/polyline",
      "files": ["or-wa.polylines"]
    },
    "whosonfirst": {
      "datapath": "/mnt/pelias/whosonfirst",
      "importVenues": false
    },
    "geonames": {
      "datapath": "./data",
      "countryCode": "US"
    }
  }
}
